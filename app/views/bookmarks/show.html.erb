<p id="notice"><%= notice %></p>


<p>
  <b>Bookmarks Id :</b>
  <%= @bookmark.id %>
</p>

<p>
  <b>Bookmarks category:</b>

 <%  if @bookmark.bookmarks_category.nil? %>

<%  else %>
    <%= @bookmark.bookmarks_category.id %>
    <%= @bookmark.bookmarks_category.name  %>
<%  end %>


</p>

<p>
  <b>Item:</b>
  <%  if @bookmark.bookmarks_category.item.nil? %>
    
    

<%  else %>
    <%= @bookmark.bookmarks_category.item_id %>
    <%= @bookmark.bookmarks_category.item.name %>
<%  end %>


</p>

<p>
  <b>Bookmark url:</b>
  <%= @bookmark.bookmark_url.to_s %>
</p>

<p>
  <b>Title:</b>
  <%= @bookmark.title %>
</p>

<p>
  <b>I frame:</b>
  <%= @bookmark.i_frame %>
</p>

<p>
  <b>Image name:</b>
  <%= @bookmark.image_name %>
 <%= image_tag @bookmark.image_name.url.to_s %>
</p>

<p>
  <b>Image name desc:</b>
  <%= @bookmark.image_name_desc.to_s %>
  <%= image_tag @bookmark.image_name_desc.url.to_s %>
</p>

<p>
  <b>Description:</b>
  <%= @bookmark.description %>
</p>

<p>
  <b>Approved </b>
  <%= @bookmark.approval %>
</p>

<%  if @bookmark.approval == 'y' %>
    <b>Approved for :</b>
    <%  if @bookmark.user_bookmark == 0 %>
        <b>Everybody </b>
    <%  else %>
        <b>User only, User id:  </b>
        <%= @bookmark.user_bookmark %>
    <%  end %>
<%  end %>
<p>
  <b>Like:</b>
  <%= @bookmark.like %>
</p>


</br>
</br>
</br>




<%= link_to 'Edit', edit_bookmark_path(@bookmark) %> |
<%= link_to 'Back', bookmarks_path %>
